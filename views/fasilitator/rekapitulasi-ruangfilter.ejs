                  c3chart_ruangtahunan.resize();
              });

          }

          var ruang_tahunan2 = '#ruang_tahunan2';

          if ( $(ruang_tahunan2).length ) {

              var c3chart_ruangtahunan2 = c3.generate({
                  bindto: ruang_tahunan2,
                  data: {
                      x : 'x',
                      columns: [
                          ['Level 2', <% rooms2.forEach((room) => { %>'<%=  room.bobotruang %>',<% }) %>],
                          ['x', <% rooms2.forEach((room) => { %>'<%=  room.dataValues.bulan %>',<% }) %>],
                      ]
                  },
                  axis: {
                      x: {
                          label: 'tanggal',
                          type: 'category',
                          position: 'outer-center',
                      },
                      y:{
                        label: 'Akumulasi nilai',
                        position: 'outer-center'
                      }
                  },
                  color: {
                      pattern: ['#FB8C00', '#FB8C00']
                  }
              });

              $window.on('debouncedresize', function () {
                  c3chart_ruangtahunan2.resize();
              });

          }



      },
    };


    </script>

