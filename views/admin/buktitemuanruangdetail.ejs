<!doctype html>
<!--[if lte IE 9]> <html class="lte-ie9" lang="en"> <![endif]-->
<!--[if gt IE 9]><!--> <html lang="en"> <!--<![endif]-->
<head>
  <%- include('head'); %>


    <!-- matchMedia polyfill for testing media queries in JS -->
    <!--[if lte IE 9]>
        <script type="text/javascript" src="bower_components/matchMedia/matchMedia.js"></script>
        <script type="text/javascript" src="bower_components/matchMedia/matchMedia.addListener.js"></script>
    <![endif]-->

</head>
<body class=" top_menu">
    <!-- main header -->

    <%- include('header'); %>

<!-- main header end -->

    <!-- main sidebar -->
    <%- include('sidebar_switch'); %>

    <div id="page_content">

        <div id="page_heading" data-uk-sticky="{ top: 48, media: 960 }">
            <h1> <%=pageTitle%> </h1>
            <span class="uk-text-upper uk-text-small"><a href="/admin/buktitemuanruang">Bukti Temuan</a> / <a href="#"> <%= rooms.penilaian_ruang.ruang.nama_ruang %></a></span>
        </div>

        <div id="page_content_inner">

            <div class="md-card">
                <div class="md-card-content">

                    <hr/>
                    <div class="uk-grid uk-grid-divider" data-uk-grid-margin>
                        <div class="uk-width-medium-2-5">
                          <h2>BUKTI TEMUAN</h2>
                          <br>
                            <div class="uk-margin-large-bottom">
                                <h2 class="heading_c uk-margin-small-bottom">Deskripsi Penemu </h2>
                                <%= rooms.deskripsi_sebelum %>
                            </div>
                            <% if (rooms.fotosebelum !== null) {%>

                            <div class="uk-margin-large-bottom">
                                <h2 class="heading_c uk-margin-small-bottom">Foto </h2>
                                <a href="/<%= rooms.fotosebelum%>" data-uk-lightbox="{group:'gallery'}">
                                    <img class="img_thumb" src="/<%= rooms.fotosebelum %>" alt="">
                                </a>

                            </div>
                            <% } %>

                            <div class="">
                              <form onsubmit="return confirm('Are you sure?');" action="/admin/buktitemuan/delete" method="POST">
                                <input type="hidden" name="_csrf" value="<%= csrfToken %>">

                                  <input type="hidden" value="<%= rooms.id %>" name="buktiTemuanId">
                                  <button id="delete" type="submit" class="md-btn md-bg-light-green-200" >Hapus Bukti Temuan</button>
                              </form>
                            </div>

                        </div>
                        <div class="uk-width-medium-2-5">
                          <h2>TINDAK LANJUT</h2>

                          <% if (rooms.tinjak_lanjut === 1) {%>

                            <div class="uk-margin-large-bottom">
                                <h2 class="heading_c uk-margin-small-bottom">Deskripsi PIC Ruang </h2>
                                <%= rooms.deskripsi_sesudah %>
                            </div>
                            <% if (rooms.fotosesudah !== null) {%>

                            <div class="uk-margin-large-bottom">
                                <h2 class="heading_c uk-margin-small-bottom">Foto </h2>
                                <a href="/<%= rooms.fotosesudah%>" data-uk-lightbox="{group:'gallery'}">
                                    <img class="img_thumb" src="/<%= rooms.fotosesudah %>" alt="">
                                </a>

                            </div>
                            <% } %>

                            <% } %>

                            <% if (rooms.tinjak_lanjut === 1) {%>
                            <form onsubmit="return confirm('Are you sure?');" action="/admin/tindaklanjutruang/delete" method="POST">
                              <input type="hidden" name="_csrf" value="<%= csrfToken %>">

                                <input type="hidden" value="<%= rooms.id %>" name="buktiTemuanId">
                                <button id="delete" type="submit" class="md-btn md-bg-light-green-200"> Hapus Tinjak Lanjut</button>
                            </form>
                            <% } %>


                        </div>
                        <div class="uk-width-medium-1-5">
                            <div class="uk-margin-medium-bottom">
                                <p>
                                    Deadline:
                                    <span class="uk-badge uk-badge-danger uk-text-upper uk-margin-small-left">  <%= rooms.deadline  %></span>
                                </p>
                                <p>
                                    Status:
                                    <% if ( rooms.tinjak_lanjut === 2 ) {%>

                                    <span class="uk-badge uk-badge-warning uk-text-upper uk-margin-small-left">Belum Tinjak Lanjut</span>
                                    <% } else {  %>
                                      <span class="uk-badge uk-badge-success uk-text-upper uk-margin-small-left">Sudah Tinjak Lanjut</span>

                                    <% }  %>

                                </p>
                            </div>
                            <h2 class="heading_c uk-margin-small-bottom">Details</h2>
                            <ul class="md-list md-list-addon">
                                <li>
                                    <div class="md-list-addon-element">
                                        <img class="md-user-image md-list-addon-avatar" src="/assets/img/avatars/avatar_02_tn.png" alt=""/>
                                    </div>
                                    <div class="md-list-content">
                                        <span class="md-list-heading"><%= rooms.penilaian_ruang.ruang.nama_ruang%> </span>
                                        <span class="uk-text-small uk-text-muted">Nama Ruang</span>
                                    </div>
                                </li>
                                <li>
                                    <div class="md-list-addon-element">
                                        <img class="md-user-image md-list-addon-avatar" src="/assets/img/avatars/avatar_02_tn.png" alt=""/>
                                    </div>
                                    <div class="md-list-content">
                                        <span class="md-list-heading"><%= rooms.penilaian_ruang.jadwal_piket.nik_pic_piket.nama %></span>
                                        <span class="uk-text-small uk-text-muted">Penemu</span>
                                    </div>
                                </li>
                                <li>
                                    <div class="md-list-addon-element">
                                        <img class="md-user-image md-list-addon-avatar" src="/assets/img/avatars/avatar_02_tn.png" alt=""/>
                                    </div>
                                    <div class="md-list-content">
                                        <span class="md-list-heading"><%= rooms.penilaian_ruang.ruang.pengguna.nama %></span>
                                        <span class="uk-text-small uk-text-muted">PIC Ruang</span>
                                    </div>
                                </li>
                                <li>
                                    <div class="md-list-addon-element">
                                        <img class="md-user-image md-list-addon-avatar" src="/assets/img/avatars/avatar_02_tn.png" alt=""/>
                                    </div>
                                    <div class="md-list-content">
                                        <span class="md-list-heading"><%= rooms.pengguna.nama %></span>
                                        <span class="uk-text-small uk-text-muted">Penindaklanjut</span>
                                    </div>
                                </li>
                                <li>
                                    <div class="md-list-addon-element">
                                        <i class="md-list-addon-icon material-icons">&#xE8DF;</i>
                                    </div>
                                    <div class="md-list-content">
                                        <span class="md-list-heading"><%= rooms.penilaian_ruang.jadwal_piket.tanggal%></span>
                                        <span class="uk-text-small uk-text-muted">Jadwal Piket</span>
                                    </div>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>



    <!-- google web fonts -->
    <script>
        WebFontConfig = {
            google: {
                families: [
                    'Source+Code+Pro:400,700:latin',
                    'Roboto:400,300,500,700,400italic:latin'
                ]
            }
        };
        (function() {
            var wf = document.createElement('script');
            wf.src = ('https:' == document.location.protocol ? 'https' : 'http') +
            '://ajax.googleapis.com/ajax/libs/webfont/1/webfont.js';
            wf.type = 'text/javascript';
            wf.async = 'true';
            var s = document.getElementsByTagName('script')[0];
            s.parentNode.insertBefore(wf, s);
        })();
    </script>

    <!-- common functions -->
    <script src="/assets/js/common.min.js"></script>
    <!-- uikit functions -->
    <script src="/assets/js/uikit_custom.min.js"></script>
    <!-- altair common functions/helpers -->
    <script src="/assets/js/altair_admin_common.min.js"></script>

    <!-- page specific plugins -->
    <!-- tablesorter -->
    <script src="/bower_components/tablesorter/dist/js/jquery.tablesorter.min.js"></script>
    <script src="/bower_components/tablesorter/dist/js/extras/jquery.tablesorter.pager.min.js"></script>

    <!--  issues list functions -->
    <script src="/assets/js/pages/pages_issues.min.js"></script>
    <!-- <script src="/assets/js/pages/page_contact_list.min.js"></script> -->
    <script src="/assets/js/pages/forms_file_input.min.js"></script>
    <script src="/assets/js/custom/dropify/dist/js/dropify.min.js"></script>

    <script>
        $(function() {
            // enable hires images
            altair_helpers.retina_images();
            // fastClick (touch devices)
            if(Modernizr.touch) {
                FastClick.attach(document.body);
            }
        });
    </script>


</body>
</html>
