      ruang_charts: function() {

        var responsiveOptions = [
            ['screen and (min-width: 641px) and (max-width: 1024px)', {
              showPoint: false,
              fullWidth: true,
              chartPadding: {
                  bottom: 40
              },
              axisX: {
                labelInterpolationFnc: function(value) {
                  // Will return Mon, Tue, Wed etc. on medium screens
                  return value.slice(0, 3);
                }
              }
            }],

            ['screen and (max-width: 640px)', {
              showLine: false,
              fullWidth: true,
              chartPadding: {
                  bottom: 40
              },
              axisX: {
                labelInterpolationFnc: function(value) {
                  // Will return M, T, W etc. on small screens
                  return value[0];
                }
              }
            }]
          ];

          // simple lines
          <%if (rooms1.length > 0 && path === '/mingguan') {%>
          new Chartist.Line('#chartist_ruang1_lines', {
            labels: [  <% rooms1.forEach((room) => { %>'<%=  room.jadwal_piket.tanggal %>',<% }) %>],
              series: [
                  [<% rooms1.forEach((room) => { %>'<%=  room.bobotruang %>',<% }) %>],
