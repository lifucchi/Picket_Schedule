<!doctype html>
<!--[if lte IE 9]> <html class="lte-ie9" lang="en"> <![endif]-->
<!--[if gt IE 9]><!--> <html lang="en"> <!--<![endif]-->
<head>
  <%- include('head'); %>


</head>
<body class=" top_menu">
    <!-- main header -->

      <%- include('header'); %>
    <!-- main header end -->
    <!-- main sidebar -->
        <%- include('top_bar'); %>


    <div id="page_content">

        <div id="page_heading" data-uk-sticky="{ top: 48, media: 960 }">
            <h1> <%=pageTitle%> </h1>
            <span class="uk-text-upper uk-text-small"><a href="/anggota/checklistpiket">Checklist Piket</a>
        </div>

      <% if (path === '/checklistpiketada') {%>
        <div id="page_content_inner">
            <div class="md-card">
              <% if (error_messages) { %>
                  <div class="uk-alert uk-alert-danger" data-uk-alert>
                      <a href="#" class="uk-alert-close uk-close"></a>
                      <%= error_messages %>
                  </div>
              <% } %>
              <% if (success_messages) { %>
                  <div class="uk-alert uk-alert-success" data-uk-alert>
                      <a href="#" class="uk-alert-close uk-close"></a>
                      <%= success_messages %>
                  </div>
              <% } %>
                <div class="md-card-content">
                    <div class="uk-margin-bottom" data-uk-margin>
                            <div class="uk-width-medium-1">
                              <h3> Tanggal <%= piket.tanggal  %>  </h3>
                              <p>
                                  Status:

                                  <% if (piket.status_piket === 0) {%>
                                  <span class="uk-badge uk-badge-warning uk-text-upper uk-margin-small-left">Belum Check</span>
                                  <% } else {%>
                                  <span class="uk-badge uk-badge-success uk-text-upper uk-margin-small-left">Sudah Check</span>
                                  <% } %>

                              </p>
                                <!-- </div> -->
                            </div>


                    </div>
                    <hr/>
                    <div class="uk-grid uk-grid-divider" data-uk-grid-margin>
                        <div class="uk-width-medium-1-2">

                            <div class="pricing_table pricing_table_a " data-uk-grid-margin data-uk-grid-match="{target:'.md-card-content'}">
                            <div>
                                <div class="md-card">
                                    <div class="md-card-content padding-reset">
                                        <div class="pricing_table_plan md-bg-light-green-200 ">Checklist Meja</div>

                                        <ul class="pricing_table_features">
                                            <li><strong>Sudah check </strong> <%= count[0]%></li>
                                            <li><strong>Belum check</strong> <%= count[2]%></li>

                                        </ul>
                                        <div class="pricing_table_select">
                                            <a href="/anggota/checklistmeja/<%= piket.id%>" class="md-btn md-btn-large md-bg-light-green-200">Open</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                          </div>

                        </div>
                        <div class="uk-width-medium-1-2">

                            <div class="pricing_table pricing_table_a " data-uk-grid-margin data-uk-grid-match="{target:'.md-card-content'}">
                            <div>
                                <div class="md-card">
                                    <div class="md-card-content padding-reset">
                                        <div class="pricing_table_plan md-bg-light-green-200 ">Checklist Ruang</div>

                                        <ul class="pricing_table_features">
                                            <li><strong>Sudah check</strong> <%= count[1]%></li>
                                            <li><strong>Belum check</strong> <%= count[3]%></li>

                                        </ul>
                                        <div class="pricing_table_select">
                                            <a href="/anggota/checklistruang/<%= piket.id%>"  class="md-btn md-btn-large md-bg-light-green-200">Open</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                          </div>

                        </div>

                        <%if  (piket.status_piket === 0 && count[3] === 0 && count[2] === 0 ) { %>

                        <div class="uk-width-medium-1  ">
                            <form class="" action="/anggota/checklistPiket/postCheckPic" method="post">
                              <div class="uk-input-group uk-align-center">
                                <input type="hidden" name="check" value="<%= piket.id %>">
                                <input type="hidden" name="_csrf" value="<%= csrfToken %>">
                                <button class="md-btn md-btn-warning" type="submit" name="button">Check !!</button>
                              </div>
                            </form>
                        </div>
                        <% } %>
                        <span class="uk-text-small uk-text-muted uk-align-center">Mohon klik check jika sudah menilai Ruang dan Meja </span>

                    </div>
                </div>
            </div>


        </div>
      <% }%>



    </div>



    <!-- google web fonts -->
    <script>
        WebFontConfig = {
            google: {
                families: [
                    'Source+Code+Pro:400,700:latin',
                    'Roboto:400,300,500,700,400italic:latin'
                ]
            }
        };
        (function() {
            var wf = document.createElement('script');
            wf.src = ('https:' == document.location.protocol ? 'https' : 'http') +
            '://ajax.googleapis.com/ajax/libs/webfont/1/webfont.js';
            wf.type = 'text/javascript';
            wf.async = 'true';
            var s = document.getElementsByTagName('script')[0];
            s.parentNode.insertBefore(wf, s);
        })();
    </script>

    <!-- common functions -->
    <script src="/assets/js/common.min.js"></script>
    <!-- uikit functions -->
    <script src="/assets/js/uikit_custom.min.js"></script>
    <!-- altair common functions/helpers -->
    <script src="/assets/js/altair_admin_common.min.js"></script>

    <!-- page specific plugins -->
    <!-- tablesorter -->
    <script src="/bower_components/tablesorter/dist/js/jquery.tablesorter.min.js"></script>
    <script src="/bower_components/tablesorter/dist/js/extras/jquery.tablesorter.pager.min.js"></script>

    <!--  issues list functions -->
    <script src="/assets/js/pages/pages_issues.min.js"></script>
    <!-- <script src="/assets/js/pages/page_contact_list.min.js"></script> -->
    <script src="/assets/js/pages/forms_file_input.min.js"></script>
    <script src="/assets/js/custom/dropify/dist/js/dropify.min.js"></script>




    <script>
        $(function() {
            // enable hires images
            altair_helpers.retina_images();
            // fastClick (touch devices)
            if(Modernizr.touch) {
                FastClick.attach(document.body);
            }
        });
    </script>


</body>
</html>
