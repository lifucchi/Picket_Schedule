    <div id="page_content">
        <div id="page_content_inner">
            <div class="md-card">
                <div class="md-card-content">
                    <div class="uk-grid" data-uk-grid-margin="">
                        <div class="uk-width-medium-3-10 ">
                          <form class="uk-form-stacked" action="/anggota/rekapitulasi/ruang/filter-mingguan" method="POST">
                            <label for="kUI_datepicker_a" class="uk-form-label">Mingguan</label>
                            <input name="tanggal" id="kUI_datepicker_a" value="" />
                            <!-- <div class="uk-width-medium-8-10 uk-text-center"> -->
                                <button type="submit" class="md-btn md-btn-primary uk-margin-small-top">Filter</button>
                                <p class="uk-text-muted">Pilih tanggal yang mewakili minggu tersebut</p>
                                <input type="hidden" name="_csrf" value="<%= csrfToken %>">


                            <!-- </div> -->
                          </form>
                        </div>
                        <div class="uk-width-medium-3-10">
                          <form class="uk-form-stacked" action="/anggota/rekapitulasi/ruang/filter-bulanan" method="POST">
                            <label for="kUI_datepicker_b" class="uk-form-label">Bulanan</label>
                            <input name="tanggal" id="kUI_datepicker_b" value="" />
                            <!-- <div class="uk-width-medium-8-10 uk-text-center"> -->
