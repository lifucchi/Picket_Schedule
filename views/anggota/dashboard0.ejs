          <div class="md-card  md-bg-light-green-200">
            <div class="md-card-content">
              <div class="gallery_grid uk-grid-width-medium-1-4 uk-grid-width-large-1-5" data-uk-grid="{gutter: 16}">
                <% articles.forEach((article) => { %>
                      <div>
                          <div class="md-card md-card-hover">
                              <div class="gallery_grid_item md-card-content">
                                  <a href="/<%= article.foto_Artikel%>" data-uk-lightbox="{group:'gallery'}">
                                      <img src="/<%= article.foto_Artikel%>" alt="">
                                  </a>
                                  <div class="gallery_grid_image_caption">
                                      <span class="gallery_image_title "><%= article.judul%></span>
                                      <br>
                                      <span class="uk-text-muted uk-text-truncate uk-text-small uk-float-right"><%= article.pembuat%></span>
                                  </div>
                              </div>
                              <div class="blog_list_footer">
                                  <a id="read" href="/fasilitator/artikel/<%= article.id%>" class="md-btn md-btn-small md-btn-flat md-btn-flat-primary uk-float-right">Read more</a>
                                  <div class="uk-modal" id="modal_artikel">
                                      <div class="uk-modal-dialog ">
                                          <button type="button" class="uk-modal-close uk-close uk-close-alt"></button>
                                          <div class="uk-overflow-container">
                                          <h2 id="judul" class="heading_b"><%= article.judul%></h2>
                                          <img id="gambar" src="/<%= article.foto_Artikel%>" alt=""/>
                                          <p id="konten"><%= article.konten%></p>
                                        </div>
                                      </div>
                                  </div>
                              </div>
                          </div>
                      </div>
<%                     }) %>
                    </div>
            </div>
          </div>




