<!doctype html>
<!--[if lte IE 9]> <html class="lte-ie9" lang="en"> <![endif]-->
<!--[if gt IE 9]><!--> <html lang="en"> <!--<![endif]-->
<head>
    <%- include('head'); %>

    <!-- additional styles for plugins -->
    <!-- metrics graphics (charts) -->

    <!-- chartist -->
    <link rel="stylesheet" href="/bower_components/chartist/dist/chartist.min.css">


</head>
<body class="top_menu md-bg-grey-200">
    <!-- main header -->
    <%- include('header'); %>
    <!-- main header end -->
    <!-- main sidebar -->
    <%- include('top_bar'); %>

    <!-- main sidebar end -->
    <div id="page_content">
        <div id="page_content_inner">
            <div class="md-card">
                <div class="md-card-content">
                    <div class="uk-grid" data-uk-grid-margin="">

                        <div class="uk-width-medium-3-10">
                          <form class="uk-form-stacked" action="/anggota/rekapitulasi/meja/filter-bulanan" method="POST">
                            <label for="kUI_datepicker_b" class="uk-form-label">Bulanan</label>
                            <input name="tanggal" id="kUI_datepicker_b" value="" />
                            <!-- <div class="uk-width-medium-8-10 uk-text-center"> -->
                                <button type="submit" class="md-btn md-btn-primary uk-margin-small-top">Filter</button>
                                <input type="hidden" name="_csrf" value="<%= csrfToken %>">

                            <!-- </div> -->
                          </form>
                        </div>

                </div>
            </div>
        </div>

        <% if (path === '/bulanan') {%>

          <div class="uk-grid uk-grid-width-large-1 uk-grid-width-medium-1 uk-grid-medium"  data-uk-grid-margin >
              <div class="">
                <div class="md-card">
                  <div class="md-card-content uk-align-center">
                    <h2 class="uk-text-center uk-margin-bottom"> Keterangan Hasil Penilaian Meja </h2>
                    <hv>

                      <h3 class="uk-text-center">
                    	>= 4.5 Consistent Star
                      <br>
                    	>= 4 Future Star
                      <br>
                    	>= 3.5 Utility Hi-Pro
                      <br>
                    	>= 3 Contributor
                      <br>
                    	< 3 Iceberg
                      <br>

                      </h3>
                  </div>

                </div>
              </div>


              <div>
                <div class="md-card">
                  <div class="md-card-content">
                    <h2 class="uk-text-center uk-margin-bottom">Nilai Tertinggi</h2>
                    <% if (mejaTerbaik) {%>
                      <div class="uk-grid">
                          <div class="uk-width-1-2">
                            <div class="uk-panel">
                                <i href="" class="uk-align-center uk-icon-large uk-icon-button uk-icon-star md-bg-lime-A200"></i>
                                <% if (mejaTerbaik.bobotmeja >= 4.5 ) {%>
                                  <h3 class="uk-text-center"> Consistent Star </h3>
                                <% }else if (mejaTerbaik.bobotmeja >= 4){%>
                                  <h3 class="uk-text-center"> Future Star </h3>
                                <% }else if (mejaTerbaik.bobotmeja >= 3.5 ){%>
                                  <h3 class="uk-text-center"> Utility Hi-Pro </h3>

                                <% }else if (mejaTerbaik.bobotmeja >= 3){%>
                                  <h3 class="uk-text-center"> Contributor </h3>

                                <% }else if (mejaTerbaik.bobotmeja < 3){%>
                                  <h3 class="uk-text-center"> Iceberg </h3>
                                <% }%>

                                <h2 class=" uk-text-center uk-text-bold"> <%= mejaTerbaik.bobotmeja %> </h2>
                            </div>
                          </div>
                          <div class="uk-width-1-2">
                            <h3 class="uk-text-center">Level <%= mejaTerbaik.meja.pengguna.level %></h3>
                            <h3 class="uk-text-center"><%= mejaTerbaik.meja.pengguna.nama %>   </h3>
                          </div>
                      </div>
                      <% }else{ %>
                      <h3 class="uk-text-middle  uk-text-center  ">
                          belum ada
                      </h3>
                      <% } %>
                  </div>
                </div>
              </div>
            </div>



        <div class="md-card">
          <div class="md-card-content">
            <h2> <%= bulan  %>  </h2>

            <div class="uk-width-large-1" >
                <div class="md-card"  >
                    <div class="md-card-content" >
                        <h4 class="heading_c uk-margin-bottom">Gabungan</h4>
                        <%if (rooms2.length > 0 || rooms1.length > 0) {%>

                        <div style="overflow-x: scroll;">
                          <div style="width:400vh; height:500px;"  id="chartist_meja_lines_bulanan" class="chartist"></div>
                        </div>
                        <% }else{ %>
                          <h3 class="uk-text-middle  uk-text-center  ">
                              belum ada
                          </h3>
                          <% } %>

                    </div>
                </div>
            </div>

            <div class="uk-width-large-1">
                <div class="md-card">
                    <div class="md-card-content">
                        <h4 class="heading_c uk-margin-bottom">Level 1</h4>
                        <%if (rooms2.length > 0) {%>
                        <div style="overflow-x: scroll;">
                          <div style="width:400vh; height:500px;"  id="chartist_meja1_lines_bulanan" class="chartist"></div>
                        </div>
                        <% }else{ %>
                          <h3 class="uk-text-middle  uk-text-center  ">
                              belum ada
                          </h3>
                          <% } %>
                    </div>
                </div>
            </div>

            <div class="uk-width-large-1">
                <div class="md-card">
                    <div class="md-card-content">
                        <h4 class="heading_c uk-margin-bottom">Level 2</h4>

                        <%if (rooms2.length > 0) {%>

                        <div style="overflow-x: scroll;">
                          <div style="width:400vh; height:500px;" id="chartist_meja2_lines_bulanan" class="chartist"></div>
                        </div>
                        <% }else{ %>
                          <h3 class="uk-text-middle  uk-text-center  ">
                              belum ada
                          </h3>
                          <% } %>
                    </div>
                </div>
            </div>
          </div>
        </div>



        <% }%>



    </div>

    <!-- google web fonts -->
    <script>
        WebFontConfig = {
            google: {
                families: [
                    'Source+Code+Pro:400,700:latin',
                    'Roboto:400,300,500,700,400italic:latin'
                ]
            }
        };
        (function() {
            var wf = document.createElement('script');
            wf.src = ('https:' == document.location.protocol ? 'https' : 'http') +
            '://ajax.googleapis.com/ajax/libs/webfont/1/webfont.js';
            wf.type = 'text/javascript';
            wf.async = 'true';
            var s = document.getElementsByTagName('script')[0];
            s.parentNode.insertBefore(wf, s);
        })();
    </script>

    <!-- common functions -->
    <script src="/assets/js/common.min.js"></script>
    <!-- uikit functions -->
    <script src="/assets/js/uikit_custom.min.js"></script>
    <!-- altair common functions/helpers -->
    <script src="/assets/js/altair_admin_common.min.js"></script>

    <script src="/assets/js/kendoui_custom.min.js"></script>

    <!--  kendoui functions -->
    <script src="/assets/js/pages/kendoui.min.js"></script>

    <!-- d3 -->
    <!-- metrics graphics (charts) -->
    <!-- chartist -->
    <script src="/bower_components/chartist/dist/chartist.min.js"></script>

    <script type="text/javascript">

    $(function() {

        // chartist
        altair_charts.meja_charts();
    });

    altair_charts = {

      meja_charts: function() {

        var responsiveOptions = [
            ['screen and (min-width: 641px) and (max-width: 1024px)', {
              showPoint: false,
              fullHeight: true,
              fullWidth: true,
              chartPadding: {
                  right: 40
              },
              axisX: {
                labelInterpolationFnc: function(value) {
                  // Will return Mon, Tue, Wed etc. on medium screens
                  return value.slice(0, 3);
                }
              }
            }],
            ['screen and (max-width: 640px)', {
              fullHeight: true,
              showLine: false,
              fullWidth: true,
              chartPadding: {
                  right: 40
              },
              axisX: {
                labelInterpolationFnc: function(value) {
                  // Will return M, T, W etc. on small screens
                  return value[0];
                }
              }
            }]
          ];
          // simple lines
          <%if (rooms1.length > 0 || rooms2.length > 0) {%>

          new Chartist.Line('#chartist_meja_lines_bulanan', {
            labels: [  <% rooms1.forEach((room) => { %>'<%=  room.meja.pengguna.nama %>',<% }) %>
                        <% rooms2.forEach((room) => { %>'<%=  room.meja.pengguna.nama %>',<% }) %>],

              // labels: ['Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday'],
              series: [
                  [<% rooms1.forEach((room) => { %>'<%=  room.bobotmeja %>',<% }) %>
                <% rooms2.forEach((room) => { %>'<%=  room.bobotmeja %>',<% }) %>],
              ]
          }, responsiveOptions );
          <% } %>

          <%if (rooms1.length > 0) {%>

          new Chartist.Line('#chartist_meja1_lines_bulanan', {
            labels: [  <% rooms1.forEach((room) => { %>'<%=  room.meja.pengguna.nama %>',<% }) %>],

              // labels: ['Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday'],
              series: [
                  [<% rooms1.forEach((room) => { %>'<%=  room.bobotmeja %>',<% }) %>],
              ]
          }, responsiveOptions);
          <% } %>
          <%if (rooms2.length > 0) {%>

          new Chartist.Line('#chartist_meja2_lines_bulanan', {
            labels: [  <% rooms2.forEach((room) => { %>'<%=  room.meja.pengguna.nama %>',<% }) %>],

              // labels: ['Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday'],
              series: [
                  [<% rooms2.forEach((room) => { %>'<%=  room.bobotmeja %>',<% }) %>],
              ]
          }, responsiveOptions);
          <% } %>

        //   chart.on('draw', function(context) {
        //   if(context.type === 'line') {
        //     context.element.attr({
        //       style: 'stroke: hsl(' + Math.floor(Chartist.getMultiValue(context.value) / max * 100) + ', 50%, 50%);'
        //     });
        //   }
        // });

      }
    };


    </script>




    <script>
        $(function() {
            // enable hires images
            altair_helpers.retina_images();
            // fastClick (touch devices)
            if(Modernizr.touch) {
                FastClick.attach(document.body);
            }

        });


    </script>


</body>
</html>
